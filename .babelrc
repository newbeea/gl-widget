{
  "presets": [[
      "@babel/env", {
        "targets": {
            "edge": "17",
            "firefox": "60",
            "chrome": "67",
            "safari": "11.1",
            "ie": "11"
        }
      }
    ], [
      "@babel/typescript"
    ]
  ],
  "plugins": [[ 
    "@babel/plugin-transform-runtime", {
      "corejs": 3, 
      "helpers": true,
      "regenerator": true,
      "useESModules": false
    }]
  ]
    
}
  